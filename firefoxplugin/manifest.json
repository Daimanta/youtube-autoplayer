{
  "manifest_version": 2,
  "name": "Youtube Autoplayer plugin",
  "version": "1.0",

  "description": "Adds the option to send a video to the server",

  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["contextmenuchanger.js"]
    }
  ],
  "permissions": ["contextMenus", "webRequest", "webRequestBlocking"]

}
